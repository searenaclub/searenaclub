        {
          "alias": false,
          "_styles": {
            "& when not (@fullScreen)": {
              "padding-top": "(@paddingTop * 1rem)",
              "padding-bottom": "(@paddingBottom * 1rem)"
            },
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value"
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)"
            },
            ".mbr-fallback-image.disabled": {
              "display": "none"
            },
            ".mbr-fallback-image": {
              "display": "block",
              "background-size": "cover",
              "background-position": "center center",
              "width": "100%",
              "height": "100%",
              "position": "absolute",
              "top": "0",
              "& when (@bg-type = 'video')": {
                "background-image": "url(@fallBackImage)"
              }
            },
            ".content-wrapper": {
              "padding": "0 60px",
              "display": "flex",
              "@media (max-width: 992px)": {
                "display": "block",
                "padding": "0"
              },
              ".content-wrap": {
                "width": "50%",
                "padding-right": "80px",
                "@media (max-width: 1199px)": {
                  "padding-right": "32px"
                },
                "@media (max-width: 992px)": {
                  "padding-right": "0",
                  "width": "100%",
                  "margin-bottom": "32px"
                },
                "& when (@reverseContent)": {
                  "padding-right": "0",
                  "padding-left": "80px",
                  "@media (max-width: 1199px)": {
                    "padding-left": "32px"
                  },
                  "@media (max-width: 992px)": {
                    "padding-left": "0"
                  }
                },
                ".text-wrapper": {
                  "position": "sticky",
                  "top": "32px",
                  "@media (max-width: 992px)": {
                    "position": "static"
                  },
                  ".mbr-section-title": {
                    "position": "relative",
                    "z-index": "1",
                    "margin-bottom": "32px",
                    "span": {
                      "padding": "0 10px",
                      "border-radius": "8rem",
                      "background-color": "@title"
                    },
                    "@media (max-width: 992px)": {
                      "margin-bottom": "24px"
                    }
                  },
                  ".mbr-text": {
                    "position": "relative",
                    "z-index": "1",
                    "margin-bottom": "0"
                  }
                }
              },
              ".items-wrapper": {
                "padding-left": "80px",
                "border-left": "3px solid @border",
                "width": "50%",
                "@media (max-width: 1199px)": {
                  "padding-left": "32px"
                },
                "@media (max-width: 992px)": {
                  "border-left": "none",
                  "border-top": "3px solid @border",
                  "padding-left": "0",
                  "width": "100%"
                },
                "& when (@reverseContent)": {
                  "padding-left": "0",
                  "padding-right": "80px",
                  "border-left": "none",
                  "border-right": "3px solid @border",
                  "@media (max-width: 1199px)": {
                    "padding-right": "32px"
                  },
                  "@media (max-width: 992px)": {
                    "padding-left": "0",
                    "border-right": "none",
                    "border-top": "3px solid @border"
                  }
                },
                ".item": {
                  "padding": "32px 0",
                  "@media (max-width: 992px)": {
                    "padding": "32px 0 0"
                  },
                  ".item-wrapper": {
                    ".card-box": {
                      ".item-title": {
                        "margin-bottom": "16px"
                      },
                      ".item-text": {
                        "margin-bottom": "16px",
                        "@media (max-width: 992px)": {
                          "margin-bottom": "0"
                        }
                      }
                    }
                  }
                }
              }
            },
